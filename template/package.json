{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "dist/{{name}}.common.js",
  "module": "dist/{{name}}.esm.js",
  "unpkg": "dist/{{name}}.js",
  "files": [
    "src",
    "dist/{{name}}.js",
    "dist/{{name}}.min.js",
    "dist/{{name}}.esm.js",
    "dist/{{name}}.common.js"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "prepublish": "npm run build",
    "build": "node build/build",
    "lint": "eslint src examples --ext .vue,.js",
    {{#flow}}
    "flow": "flow check",
    {{/flow}}
    "test": "npm run lint {{#flow}}&& npm run flow {{/flow}}&& npm run test:unit {{#e2e}}&& npm run test:e2e{{/e2e}}",
    {{#e2e}}
    "test:e2e": "node test/e2e/runner.js",
    {{/e2e}}
    "test:unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js"
  },
  {{^if_falsy git}}
  "repository": {
    "type": "git",
    "url": "{{git}}"
  },
  {{/if_falsy}}
  "keywords": [
    "vue",
    "template"
  ],
  "author": {
    "name": "{{ authorFullNameFrom author }}",
    "email": "{{ authorEmailFrom author }}"
  },
  "license": "{{license}}",
  "dependencies": {
    "lodash": "^4.17.4",
    "vue": "latest"
  },
  "devDependencies": {
    {{#e2e}}
    "chromedriver": "^2.32.3",
    "nightwatch": "^0.9.16",
    "nightwatch-helpers": "^1.2.0",
    "selenium-server": "^3.5.3",
    "cross-spawn": "^5.1.0",
    {{/e2e}}
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/if_eq}}
    {{#flow}}
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "babel-preset-flow-vue": "^1.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "flow-bin": "^0.55.0",
    {{/flow}}
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-power-assert": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "eslint": "^4.6.1",
    "eslint-plugin-vue": "^3.13.0",
    "express": "^4.15.4",
    "express-urlrewrite": "^1.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "power-assert": "^1.4.4",
    "rollup": "^0.41.5",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-vue": "^2.5.0",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
